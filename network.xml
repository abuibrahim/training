<testbed uri="qemu:///system">

  <domain type='kvm'>
    <name>ve1</name>
    <memory unit='MiB'>1024</memory>
    <vcpu placement='static'>1</vcpu>
    <cpu mode='host-passthrough'/>
    <os>
      <type arch='x86_64'>hvm</type>
      <boot dev='hd'/>
    </os>
    <features>
      <acpi/>
      <apic/>
    </features>
    <devices>
      <disk type='file' device='disk'>
        <driver name='qemu' type='qcow2'/>
        <source file='/var/lib/libvirt/training/ve1.qcow2'/>
        <target dev='vda' bus='virtio'/>
      </disk>
      <disk type='file' device='cdrom'>
        <driver name='qemu' type='raw'/>
        <source file='/var/lib/libvirt/training/ve1.iso'/>
        <target dev='hdc' bus='ide'/>
      </disk>
      <controller type='pci' index='0' model='pci-root'/>
      <controller type='virtio-serial' index='0'/>
      <rng model='virtio'>
        <backend model='random'>/dev/random</backend>
      </rng>
      <interface type='network'>
        <source network='mgmt'/>
        <model type='virtio'/>
      </interface>
      <interface type='network'>
        <source network='gold'/>
        <model type='virtio'/>
      </interface>
      <interface type='network'>
        <source network='silver'/>
        <model type='virtio'/>
      </interface>
      <interface type='network'>
        <source network='branch-1'/>
        <model type='virtio'/>
      </interface>
      <serial type='pty'>
        <target port='0'/>
      </serial>
      <console type='pty'>
        <target type='serial' port='0'/>
      </console>
    </devices>
  </domain>

